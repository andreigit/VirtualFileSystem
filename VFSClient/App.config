<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IVFSService"/>
            </wsDualHttpBinding>
        </bindings>
        <client>
            <!-- endpoint address="http://localhost:8000/VirtualFileSystem/Service/" -->
            <endpoint address="http://localhost:8733/Design_Time_Addresses/VirtualFileSystem.Service.Model/VFSService/" binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IVFSService" contract="VFSServiceReference.IVFSService" name="WSDualHttpBinding_IVFSService">
                <identity>
                  <!--
                  <servicePrincipalName value="host/COMP-NAME"/>
                  -->
                  <servicePrincipalName value="host/localhost"/>
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
  <appSettings>
    <!--
    <add key="DefaultEndpointAuthority" value="localhost:8000"/>
    -->
    <add key="DefaultEndpointAuthority" value="localhost:8733"/>
    <add key="TaskTimeoutMilliseconds" value="1000"/>
  </appSettings>
</configuration>
